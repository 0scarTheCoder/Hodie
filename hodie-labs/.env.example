# Auth0 Configuration (Recommended Authentication)
# Get your Auth0 credentials from: https://manage.auth0.com/
REACT_APP_AUTH0_DOMAIN=your-tenant.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=https://api.hodielabs.com
REACT_APP_AUTH0_CUSTOM_DOMAIN=login.hodielabs.com

# Kimi K2 AI Configuration
# Get your free API key from one of these providers:
# - AI/ML API: https://api.aimlapi.com (Recommended)
# - OpenRouter: https://openrouter.ai  
# - Together AI: https://together.ai

# AI/ML API Configuration (Recommended)
REACT_APP_KIMI_K2_API_KEY=your_aimlapi_key_here

# Alternative: If using OpenRouter
# REACT_APP_KIMI_K2_API_KEY=your_openrouter_key_here
# REACT_APP_KIMI_K2_PROVIDER=openrouter

# Alternative: If using Together AI  
# REACT_APP_KIMI_K2_API_KEY=your_together_key_here
# REACT_APP_KIMI_K2_PROVIDER=together

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# MongoDB Configuration (optional - for advanced features)
REACT_APP_MONGODB_URI=your_mongodb_connection_string

# API Base URL (for health metrics backend)
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Development Settings
NODE_ENV=development
REACT_APP_DEBUG=false